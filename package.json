{
  "name": "npm-template-javascript",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "browserslist": [
    "ie 11",
    "ios >= 10",
    "> 1% in jp",
    "not ie <= 10",
    "not chrome 49"
  ],
  "lint-staged": {
    "subTaskConcurrency": 1,
    "linters": {
      "*.{js,jsx}": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "scripts": {
    "build": "run-s build:*",
    "build:source": "babel src -d dist",
    "build:types": "flow gen-flow-files --out-dir dist src",
    "lint": "eslint --ext .js src",
    "fix": "prettier --write src/**/*.{js,json}",
    "typecheck": "flow",
    "lint-staged": "lint-staged",
    "precommit": "run-s typecheck lint-staged"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-flow": "^7.0.0-beta.44",
    "babel-eslint": "^8.2.2",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-flowtype-errors": "^3.5.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.69.0",
    "flow-typed": "^2.4.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1"
  }
}
